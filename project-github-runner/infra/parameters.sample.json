{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "baseName": {
            "value": "gh-runner"
        },
        "tags": {
            "value": {
                "project": "github-runner"
            }
        },
        "containerImage": {
            "value": "ghcr.io/myorg/github-runner:1.0.0"
        },
        "containerCpu": {
            "value": "2.0"
        },
        "containerMemory": {
            "value": "4Gi"
        },
        "runnerLabels": {
            "value": "self-hosted,azure-container-apps"
        },
        "githubOwner": {
            "value": "myorg"
        },
        "githubRepo": {
            "value": "my-repository"
        },
        "githubPatSecretValue": {
            "value": "<replace-with-secure-pat>"
        },
        "virtualNetworkAddressPrefix": {
            "value": "10.10.0.0/16"
        },
        "containerAppsSubnetPrefix": {
            "value": "10.10.0.0/23"
        },
        "platformReservedCidr": {
            "value": "10.200.0.0/24"
        },
        "platformReservedDnsIp": {
            "value": "10.200.0.10"
        },
        "dockerBridgeCidr": {
            "value": "172.16.0.0/16"
        },
        "internalEnvironment": {
            "value": false
        },
        "deployContainerRegistry": {
            "value": true
        },
        "acrName": {
            "value": "ghrunneracr1234"
        },
        "acrSku": {
            "value": "Standard"
        }
    }
}